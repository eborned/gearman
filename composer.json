{
    "name": "websightnl/gearman",
    "type": "library",
    "description": "PHP library for dispatching, handling and managing Gearman Workers",
    "keywords": ["Gearman", "Workers", "Daemon", "Jobs", "Tasks"],
    "homepage": "https://github.com/websightnl/gearman",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Bus",
            "email": "software.support@websight.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-gearman": "*",
        "psr/log": "^3.0",
        "react/event-loop": "^1.4",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Gearman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Gearman\\Tests\\": "tests"
        }
    },
    "bin": ["bin/gearman", "bin/gearman.php"]
}
